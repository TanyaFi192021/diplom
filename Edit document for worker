Логика изменения документа
<?php
require_once 'connect.php';



$DocName = $_POST['DocName'];
$DocType = $_POST['DocType'];
$ProductArticule = $_POST['ProductArticule'];
$ProductName = $_POST['ProductName'];
$ProductType = $_POST['ProductType'];
$ProductCost = $_POST['ProductCost'];
$ProductAmount = $_POST['ProductAmount'];
$SupplyDate = $_POST['SupplyDate'];
$SupplyAddress = $_POST['SupplyAddress'];
$Description = $_POST['Description'];
$CreateDate = $_POST['CreateDate'];
$Creator = $_POST['Creator'];


$path = 'assets/' . time() . $_FILES['ProductImage']['name'];
    if(!move_uploaded_file($_FILES['ProductImage']['tmp_name'], '../' . $path))
    {
        $_SESSION['message'] = 'Ошибка при закрузке сообщения';
        header("Location: ../UpdateArchive.php");
    }

mysqli_query($connect, "UPDATE `Archive` SET `DocName` = '$DocName', `DocType` = '$DocType', `ProductArticule` = '$ProductArticule', `ProductName` = '$ProductName', `ProductType` = '$ProductType', `ProductCost` = '$ProductCost', `ProductAmount` = '$ProductAmount', `SupplyDate` = '$SupplyDate', `SupplyAddress` = '$SupplyAddress', `Description` = '$Description', `ProductImage` = '$path', `CreateDate` = '$CreateDate', `Creator` = '$Creator' WHERE `Archive`.`ProductArticule` = '$ProductArticule'");




header ("Location: ../ArchiveWorker.php");
