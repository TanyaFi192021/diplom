Логика редактирования
<?php
require_once 'connect.php';



$NameDoc = $_POST['NameDoc'];
$TypeDoc = $_POST['TypeDoc'];
$ArticuleProduct = $_POST['ArticuleProduct'];
$NameProduct = $_POST['NameProduct'];
$TypeProduct = $_POST['TypeProduct'];
$CostProduct = $_POST['CostProduct'];
$AmountProduct = $_POST['AmountProduct'];
$DateSupply = $_POST['DateSupply'];
$AddressSupply = $_POST['AddressSupply'];
$TitleProduct = $_POST['TitleProduct'];
$DateCreateDoc = $_POST['DateCreateDoc'];
$Creater = $_POST['Creater'];


$path = 'assets/' . time() . $_FILES['ImageProduct']['name'];
    if(!move_uploaded_file($_FILES['ImageProduct']['tmp_name'], '../' . $path))
    {
        $_SESSION['message'] = 'Ошибка при закрузке сообщения';
        header("Location: ../UpdateDocument.php");
    }

mysqli_query($connect, "UPDATE `Document` SET `NameDoc` = '$NameDoc', `TypeDoc` = '$TypeDoc', `ArticuleProduct` = '$ArticuleProduct', `NameProduct` = '$NameProduct', `TypeProduct` = '$TypeProduct', `CostProduct` = '$CostProduct', `AmountProduct` = '$AmountProduct', `DateSupply` = '$DateSupply', `AddressSupply` = '$AddressSupply', `TitleProduct` = '$TitleProduct', `ImageProduct` = '$path', `DateCreateDoc` = '$DateCreateDoc', `Creater` = '$Creater' WHERE `Document`.`ArticuleProduct` = '$ArticuleProduct'");




header ("Location: ../ArchiveProvider.php");
