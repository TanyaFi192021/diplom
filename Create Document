Логика создания документа
<?php
session_start();
require_once 'connect.php';

$IDDoc = $_POST['IDDoc'];
$NameDoc = $_POST['NameDoc'];
$TypeDoc = $_POST['TypeDoc'];
$ArticuleProduct = $_POST['ArticuleProduct'];
$NameProduct = $_POST['NameProduct'];
$TypeProduct = $_POST['TypeProduct'];
$CostProduct = $_POST['CostProduct'];
$AmountProduct = $_POST['AmountProduct'];
$DateSupply = $_POST['DateSupply'];
$AddressSupply = $_POST['AddressSupply'];
$TitleProduct = $_POST['TitleProduct'];
$DateCreateDoc = $_POST['DateCreateDoc'];
$Creater = $_POST['Creater'];


$path = 'assets/' . time() . $_FILES['ImageProduct']['name'];
if(!move_uploaded_file($_FILES['ImageProduct']['tmp_name'], '../' . $path))
    {
        $_SESSION['message'] = 'Ошибка при закрузке картинки';
    }

mysqli_query($connect, "INSERT INTO `Document`
(`IDDoc`, `NameDoc`, `TypeDoc`, `ArticuleProduct`, `NameProduct`,
`TypeProduct`, `CostProduct`, `AmountProduct`, `DateSupply`, `AddressSupply`, `TitleProduct`, `ImageProduct`, `DateCreateDoc`, `Creater`)
VALUES (NULL, '$NameDoc', '$TypeDoc', '$ArticuleProduct', '$NameProduct', '$TypeProduct', '$CostProduct', '$AmountProduct', '$DateSupply', '$AddressSupply', '$TitleProduct', '$path', '$DateCreateDoc', '$Creater')");

header("Location: ../PageProvider.php");

?>
