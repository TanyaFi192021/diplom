Логика отправления документа
<?php
session_start();
require_once 'connect.php';

$IDNote = $_POST['IDDoc'];
$DocName = $_POST['NameDoc'];
$DocType = $_POST['TypeDoc'];
$ProductArticule = $_POST['ArticuleProduct'];
$ProductName = $_POST['NameProduct'];
$ProductType = $_POST['TypeProduct'];
$ProductCost = $_POST['CostProduct'];
$ProductAmount = $_POST['AmountProduct'];
$SupplyDate = $_POST['DateSupply'];
$SupplyAddress = $_POST['AddressSupply'];
$Description = $_POST['TitleProduct'];
$CreateDate = $_POST['DateCreateDoc'];
$Creator = $_POST['Creater'];


$path = 'assets/' . time() . $_FILES['ImageProduct']['name'];
if(!move_uploaded_file($_FILES['ImageProduct']['tmp_name'], '../' . $path))
    {
        $_SESSION['message'] = 'Ошибка при закрузке картинки';
    }

mysqli_query($connect, "INSERT INTO `Archive` (`IDNote`, `DocName`, `DocType`, `ProductArticule`, `ProductName`, `ProductType`, `ProductCost`, `ProductAmount`, `SupplyDate`, `SupplyAddress`, `Description`, `ProductImage`, `CreateDate`, `Creator`) VALUES (NULL, '$DocName', '$DocType', '$ProductArticule', '$ProductName', '$ProductType', '$ProductCost', '$ProductAmount', '$SupplyDate', '$SupplyAddress', '$Description', '$path', '$CreateDate', '$Creator')");

header("Location: ../ArchiveProvider.php");

?>
