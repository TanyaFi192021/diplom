<body>
          <?php
          $ProductArticule = $_GET['ProductArticule'];
          $note = mysqli_query($connect, "SELECT * FROM `Archive` WHERE `ProductArticule` = '$ProductArticule'");
          $note = mysqli_fetch_assoc($note);
          ?>

          <table border="2">
            <tr>
              <td colspan="2" style="text-align: center;"><b><?= $note['DocName'] ?></b></td>
            </tr>

