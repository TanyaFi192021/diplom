    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js"></script>
</head>
<body>
    <header>
        <div id="logo">
            <span><a href="MainPage.php">DocInter</a></span>
        </div>
    </header>
    <div id="clear"></div><br>
    <div>
        <form class="decor" action="vendor/signup.php" method="post" enctype="multipart/form-data">
            <div class="circle"></div>
            <div class="form-inner">
            <h3>Регистрация</h3>
            <label>Фамилия</label>
            <input type="text" name="SurnameUser" placeholder="Введите фамилию" required>
            <label>Имя</label>
            <input type="text" name="NameUser" placeholder="Введите имя" required>
            <label>Отчество</label>
            <input type="text" name="PatronumicUser" placeholder="Введите отчество">
            <label>Номер телефона</label>
            <input type="tel" name="PhoneNumberUser" placeholder="Введите номер телефона" required>
            <label>Почта</label>
            <input type="text" name="EmailUser" placeholder="Введите почту" required>
            <label>Пароль</label>
            <input type="password" name="PasswordUser" placeholder="Введите пароль" required>
            <label>Подтверждение пароля</label>
            <input type="password" name="password_confirm" placeholder="Подтвердите пароль" required>
            <label>Фото работника</label>
            <input type="file" name="ImageUser" required>
            <button type="submit">Регистрация</button><br>
            <span>Уже есть аккаунт? <b><a href="AuthorizeWorker.php">Войти</b></a></span>
            <?php
            if($_SESSION['message'])
            {
                echo '<p class="msg"> '. $_SESSION['message'] .' </p>';
            }
            unset($_SESSION['message']);
            ?> 
            </div>
        </form>
    </div> 
</body>
</html>

