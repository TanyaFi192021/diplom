      <div class="content">

        <table>
          <th></th>
          <th>Название организации</th>
          <th>Номер телефон</th>
          <th>Почта</th>
          <th></th>

        <?php
        $provider = mysqli_query($connect, "SELECT `ImageProvider`, `NameOrg`, `PhoneNumberProvider`, `EmailProvider` FROM `Provider`");
        $provider = mysqli_fetch_all($provider);
        foreach ($provider as $provider) {
          ?>

          <tr>
            <td><img src="<?= $provider[0] ?>" width="100"></td>
            <td><?= $provider[1] ?></td>
            <td><?= $provider[2] ?></td>
            <td><?= $provider[3] ?></td>
            <td><a href="vendor/delete1.php?EmailProvider=<?= $provider[3] ?>"><input type="button" class="btn" value="Удалить"></input></a></td>
            <td><a href="Update1.php?EmailProvider=<?= $provider[3] ?>"><input type="button" class="btn" value="Редактировать"></input></a></td>
          </tr>

          <?php
        }
        ?>
        </table>
      </div>
</body>
